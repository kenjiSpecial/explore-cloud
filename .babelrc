{
	"env": {
		"development": {
			"presets": ["next/babel"],
			"plugins": [
				[
					"module-resolver",
					{
						"root": ["./"],
						"alias": {
							"@components": "./src/components",
							"@utils": "./src/utils",
							"@layout": "./src/layout",
							"@styles/*": "./src/styles/*"
						}
					}
				]
			]
		},
		"production": {
			"presets": ["next/babel"],
			"plugins": [
				[
					"module-resolver",
					{
						"root": ["./"],
						"alias": {
							"@components": "./src/components",
							"@utils": "./src/utils",
							"@layout": "./src/layout",
							"@styles/*": "./src/styles/*"
						}
					}
				]
			]
		}
	}
}
